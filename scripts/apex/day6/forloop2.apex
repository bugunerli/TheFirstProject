/**
Count from 10 from to 1 using for loop


 */

 for(Integer count=10; count>0; count--){
    System.debug(count);
 }

System.debug('-------------------------');
/**
Count down from 100 to 0 decreasing number by 10 each time
 */
 for(Integer count2=100; count2>=0; count2-=10){
    System.debug(count2);
 }